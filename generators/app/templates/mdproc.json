{
	"target_dir": "out",
	"default_formats": [ "html" ],
	"markdown_files": [
		"README.md",
		"src/*.md"
	],
	"image_files": [
		"src/images/*"
	],
	"watched_files": [
		"README.md",
		"src/**/*.md"
	]<% if (supportPdf) { %>,
