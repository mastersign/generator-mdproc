{
	"target_dir": "out",
	"default_formats": [ "html" ],
	"markdown_files": [
		"README.md",
		"src/*.md"
	],
	"md2html_options": {
		"theme": "<%= htmlTheme.toLowerCase() %>"
	},
	"md2docx_options": { },
<% if (supportPdf) { %>	"md2tex_options": { },
	"md2pdf_options": { },
<% } %>	"image_files": [
		"src/img/*",
		"src/img-gen/*"
	],
	"watched_files": [
		"README.md",
		"src/**/*.md"
	]<% if (supportPdf) { %>,
	"latex_vars": {
		"fontsize": "11pt",
		"papersize": "a4paper",
		"linkColor": "ForestGreen",
		"classoption": [],
		"header-includes": [ "\\pagestyle{headings}" ]
	}<% } %>
}
